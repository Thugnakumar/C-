#include <iostream>
#include <cstring>
#include "queue.h"
#include "node.h"

using namespace std;

queue::queue(){
}

bool queue::enqueue(node * &head, node * node){
  if (head == NULL){
    head = node;
  }

  if (node != head){
    node->next = head;
    head = node;
  }

  return true;
}

bool queue::dequeue(node * &head, node * current){
  current = head;
  while (current->next != NULL){
    current = current->next;
  }
  delete current;
  return true;
}

char stack::peek(node * head){
  return head->data;
}

void queue::print(node * head){
  while (head->data != '\0'){
    cout << head->data;
    head = head->next;
  }
}

stack::~stack(){
}





